---
import type { GetStaticPaths } from "astro";
import Layout from "../../layouts/Layout.astro";

export function getStaticPaths() {
  return [
    { params: { year: "2025" }},
  ];
}

const { year } = Astro.params;
---

<Layout page_title={year}>
    <main data-pagefind-body>
        <h1>{year} website</h1>

        <iframe src={`https://${year}.mhsroboboat.com/`} title={`${year} website`} />
    </miin>
</Layout>

<style>
    main {
        width: unset !important;
        max-width: unset !important;
        padding: 1em 0 0 0;

        display: flex;
        flex-direction: column;
        align-items: center;
    }

    iframe {
        width: 100%;
        height: 100vh;
        border: none;
        border-radius: 10px;
    }
</style>